{
  "name": "form-frontend",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.20.15",
    "@types/react": "^16.14.10",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint-config-prettier": "^8.1.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^4.0.0",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.1",
    "react-icons": "^3.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^0.2.2"
  },
  "scripts": {
    "gen:theme-typings": "chakra-cli tokens src/theme/index.ts",
    "postinstall": "npm run gen:theme-typings",
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired build",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-app-rewired test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint ./ --ignore-path .gitignore && prettier . -c",
    "lint:fix": "eslint ./ --ignore-path .gitignore --fix && prettier . -c --write",
    "test:a11y": "npm run build-storybook && jest --config .storybook/storyshots/jest.config.js",
    "test:a11y-dev": "cross-env USE_DEV_SERVER=true jest --config .storybook/storyshots/jest.config.js",
    "pre-commit": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.4.0",
    "@storybook/addon-a11y": "^6.3.2",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-storyshots": "^6.3.2",
    "@storybook/addon-storyshots-puppeteer": "^6.3.2",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.2",
    "@storybook/theming": "^6.3.2",
    "@types/react-router-dom": "^5.1.7",
    "chromatic": "^5.9.2",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "lint-staged": "^10.5.4",
    "puppeteer": "^9.1.1",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "ts-jest": "^26.5.6"
  }
}
